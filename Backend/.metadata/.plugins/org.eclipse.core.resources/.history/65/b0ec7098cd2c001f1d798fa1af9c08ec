package com.jobcategoryservice.exception;

import org.springframework.http.HttpStatus;
import org.springframework.web.bind.annotation.ResponseStatus;

@ResponseStatus(HttpStatus.NOT_FOUND)
public class JobCategoryIdAlreadyExistsException extends Exception {
	
	public JobCategoryIdAlreadyExistsException(String message) {
		super(message);
	}

}
